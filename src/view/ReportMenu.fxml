<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="466.0" prefWidth="703.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ReportMenu">
    <children>
        <VBox layoutX="20.0" layoutY="31.0" prefHeight="397.0" prefWidth="667.0" spacing="15.0">
            <children>
            <Label text="Type by Month Report:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <HBox alignment="CENTER_LEFT" nodeOrientation="LEFT_TO_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="15.0">
               <children>
                  <ComboBox fx:id="MonthComboLbl" onAction="#OnActionMonthCombo" prefWidth="150.0" promptText="Select Month" />
                  <ComboBox fx:id="TypeComboLbl" layoutX="10.0" layoutY="10.0" onAction="#onActionTypeCombo" prefWidth="150.0" promptText="Select Type" />
                  <Label fx:id="reportOneLbl" />
               </children>
            </HBox>
              <Label text="Contact Schedules Report:">
                  <font>
                      <Font name="System Bold" size="12.0" />
                  </font>
              </Label>
                <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="12.0">
                    <children>
                  <ComboBox fx:id="contactComboLbl" onAction="#onActionContactCombo" prefHeight="25.0" prefWidth="184.0" promptText="Select Contact" />
                        <TableView fx:id="contactTableView" prefHeight="100.0" prefWidth="475.0">
                            <columns>
                                <TableColumn fx:id="appIDCol" prefWidth="95.0" text="Appt. ID" />
                                <TableColumn fx:id="descCol" prefWidth="95.0" text="Desc." />
                                <TableColumn fx:id="startCol" prefWidth="95.0" text="Start Date/Time" />
                                <TableColumn fx:id="endCol" prefWidth="95.0" text="End Date/Time" />
                                <TableColumn fx:id="custIDCol" prefWidth="95.0" text="Cust. ID" />
                            </columns>
                        </TableView>
                    </children>
                </HBox>
            <Label text="Extra Report Session Stats:">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <GridPane prefWidth="572.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="329.0" minWidth="10.0" prefWidth="243.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="424.0" minWidth="10.0" prefWidth="424.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Number of New Customer/Appointments:" />
                  <Label text="Number of Customer/Appointment Updates:" GridPane.rowIndex="1" />
                  <Label fx:id="newCACounter" GridPane.columnIndex="1" />
                  <Label fx:id="upCACounter" GridPane.columnIndex="1" GridPane.rowIndex="1" />
               </children>
            </GridPane>
            </children>
        </VBox>
      <Button layoutX="554.0" layoutY="416.0" mnemonicParsing="false" onAction="#onActionBackBttn" text="Back" />
    </children>
</AnchorPane>
