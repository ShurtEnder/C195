<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.AddCustomer">
   <children>
      <VBox alignment="CENTER" layoutX="50.0" layoutY="67.0" prefHeight="300.0" prefWidth="500.0">
         <children>
            <GridPane prefHeight="209.0" prefWidth="500.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="245.0" minWidth="10.0" prefWidth="99.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="427.0" minWidth="10.0" prefWidth="401.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Customer ID:" />
                  <Label text="Customer Name:" GridPane.rowIndex="1" />
                  <Label text="Address:" GridPane.rowIndex="2" />
                  <Label text="Postal Code:" GridPane.rowIndex="3" />
                  <Label text="Phone Number:" GridPane.rowIndex="4" />
                  <TextField fx:id="addCustIDTxt" disable="true" editable="false" maxWidth="150.0" GridPane.columnIndex="1" />
                  <TextField fx:id="addCustNameTxt" maxWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="addCustAddressTxt" maxWidth="250.0" prefHeight="25.0" prefWidth="250.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <TextField fx:id="addCustPCodeTxt" maxWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                  <TextField fx:id="addCustPNumberTxt" maxWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <HBox alignment="CENTER" prefHeight="50.0" prefWidth="500.0" spacing="20.0">
               <children>
                  <ComboBox prefWidth="150.0" promptText="Select Country" />
                  <ComboBox prefHeight="25.0" prefWidth="178.0" promptText="Select  First Level Division" />
               </children>
            </HBox>
            <HBox alignment="BOTTOM_RIGHT" prefHeight="27.0" prefWidth="500.0" spacing="15.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#onActionAddCustSaveBttn" text="Save" />
                  <Button mnemonicParsing="false" onAction="#onActionAddCustCancelBttn" text="Cancel" />
               </children>
            </HBox>
         </children></VBox>
      <Label layoutX="36.0" layoutY="33.0" text="Add Customer">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
