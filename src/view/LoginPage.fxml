<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LoginPage">
    <children>
        <VBox alignment="CENTER" layoutX="119.0" layoutY="1.0" prefHeight="400.0" prefWidth="336.0" spacing="50.0">
            <children>
                <Label fx:id="loginPageLbl" alignment="CENTER" text="Login Page">
                    <font>
                        <Font name="System Bold" size="18.0" />
                    </font>
                </Label>
                <GridPane alignment="CENTER" prefHeight="60.0" prefWidth="522.0">
                    <columnConstraints>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                    </rowConstraints>
                    <children>
                        <TextField fx:id="userNameTxt" GridPane.columnIndex="1" />
                        <TextField fx:id="passwordTxt" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                        <Label fx:id="userNameLbl" alignment="CENTER" text="Username :">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font></Label>
                        <Label fx:id="passwordLbl" text="Password :" GridPane.rowIndex="1">
                            <font>
                                <Font name="System Bold" size="12.0" />
                            </font></Label>
                    </children>
                </GridPane>
                <Button fx:id="loginBttnTxt" mnemonicParsing="false" onAction="#onActionLoginBttn" text="Login" />
            <Label fx:id="zoneIDLbl" />
            </children>
        </VBox>
    </children>
</AnchorPane>
